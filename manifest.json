{
  "manifest_version": 2,
  "name": "My Extension",
  "version": "1.0",

  "permissions": ["tabs", "activeTab", "downloads"],

  "browser_action": {
    "default_title": "My Extension",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_popup": "popup.html"
  }
}
